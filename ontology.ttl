###  https://purl.org/fisa#BuildingClass2
:BuildingClass2 rdf:type owl:Class ;
                rdfs:subClassOf :BuildingClass ,
                                [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                         owl:onProperty :hasAverageHeight ;
                                                         owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                                             owl:onDatatype xsd:double ;
                                                                             owl:withRestrictions ( [ xsd:maxInclusive "7.0"^^xsd:double
                                                                                                    ]
                                                                                                  )
                                                                           ]
                                                       ]
                                                       [ rdf:type owl:Restriction ;
